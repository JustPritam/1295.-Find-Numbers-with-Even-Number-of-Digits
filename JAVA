class Solution {
    public int findNumbers(int[] nums) {
        int c=0,t=0,p=0;
        for(int i=0;i<nums.length;i++){
            c=0;
            while(nums[i]>0){
                t=nums[i]%10;
                c++;
                nums[i]=nums[i]/10;
            }
            if((c&1)!=1)
                p++;
        } 
        return p;
    }
}
